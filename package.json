{
    "name": "sistema-operante-auth",
    "version": "1.0.2",
    "description": "Sistema de autenticação para Sistema Operante - Aero Festas",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "build": "npx prisma generate",
        "create-admin": "node scripts/create-admin.js",
        "version": "node scripts/update-version.js && git add version.json",
        "release": "npm version patch && git push origin main --tags",
        "release:minor": "npm version minor && git push origin main --tags",
        "release:major": "npm version major && git push origin main --tags"
    },
    "keywords": [
        "auth",
        "login",
        "express"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^5.10.2",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.3",
        "nodemailer": "^7.0.11",
        "web-push": "^3.6.7"
    },
    "devDependencies": {
        "nodemon": "^3.0.1",
        "prisma": "^5.10.2"
    }
}
